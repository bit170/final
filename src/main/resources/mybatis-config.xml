<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	
	<typeAliases>
		<!-- 이메일 인증받은 회원(로그인 가능한 회원) 정보 처리용 -->
    	<typeAlias type="com.spring.biz.member.S_MemberVO" alias="svo"/>
    	
    <!-- 마이페이지 관련 alias 추가 (송희) -->
		 <typeAlias alias="pvo" type="com.spring.biz.product.ProductVO" /> 
        <!-- <typeAlias alias="member" type="com.spring.biz.member.MemberVO"/> -->
        
    <!-- 마이페이지 정보 수정 (연희) -->
		<typeAlias alias="member" type="com.spring.biz.member.MemberVO" />
		<typeAlias alias="address" type="com.spring.biz.address.AddressVO" />
		
	<!-- 작가페이지 관련 alias 추가 (서현) -->
		<typeAlias alias="artist" type="com.spring.biz.artist.ArtistVO"/>

		<!-- <typeAlias alias="wishlist" type="com.spring.biz.wishlist.WishlistVO"></typeAlias> -->

	</typeAliases>
	
</configuration>








