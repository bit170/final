<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	
	<typeAliases>
		<typeAlias alias="address" type="com.spring.biz.address.AddressVO"></typeAlias>
		<typeAlias alias="follow" type="com.spring.biz.follow.FollowVO"></typeAlias>
		<typeAlias alias="step" type="com.spring.biz.step.StepVO"></typeAlias>
		<typeAlias alias="wishlist" type="com.spring.biz.wishlist.WishlistVO"></typeAlias>
	</typeAliases>
	
  <mappers>
    <mapper resource="mappings/**mapper.xml"/>
  </mappers>
  
</configuration>








